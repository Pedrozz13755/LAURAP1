<section class="py-5">
    <div class="container">
            <center>            
                        
								<%- include('./layouts/msg')  %>
								<%- include('./layouts/errors')  %>
              
        </center>
        <div class="row">
            <div class="col-md-6 mx-auto text-center">
                <h2 class="text-white heading-black text-primary">Lojinha da api</h2>
                <p class="text-light lead mb-5" style="font-style: italic;">Bem Vindo <% if (admin === 'sim') { %>
                <strong><span class="title-word title-word-1"><%= nome_usuario %></span></strong>  <img src="img/verificado-coder.gif" alt="verificado" height="20" width="20">
                <% } else if (premium !== null) { %>
                <%= nome_usuario %>  <img src="img/marca-de-verificacao.gif" alt="verificado" height="20" width="20">
                <% } else { %>
                <%= nome_usuario %>
                <% } %></p>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <div class="card">
                    <div class="card-body">
                    
                    <h5> FAÇA UMA DOAÇÃO DO SEU DINHEIRO PARA ALGUÉM ☺️</h5> 
                        
                        <div class="text-center"> 
                            <form action="/dinheiro" method="post">
                                <div class="form-group">
                                    <input style="font-family:Arial, FontAwesome" name="username" type="text" class="form-control" placeholder="&#xf007;   Nome do Usuario" required>
                                </div>
                                  <div class="form-group">
                                    <input style="font-family:Arial, FontAwesome" name="quantia" type="number" class="form-control" placeholder="&#xf0d6;   Quantidade" required>
                                </div>
                                <div class="text-center">
                                    <button type="submit" class="btn btn-primary mb-2"><i class="fas fa-money-bill-wave"></i><em class="ml-2"></em>Transferir</button>
                                  <p class="text-muted small-xl">❗esteja ciente que ao apertar em transferir, você não poderar ter seu dinheiro de volta❗</p>               
                                </div>
                            </form>            
                        <div class="text-center"> 
                        <h5>COMPRAR PREMIUM</h5>
                            <form action="/dinheiro/buyprem" method="post">
                                  <div class="form-group">
                                    <input style="font-family:Arial, FontAwesome" name="prembuy" type="number" class="form-control" placeholder="&#xf0d6;   Quantidade" value='15000' required>
                                </div>
                                <div class="text-center">
                                    <button type="submit" class="btn btn-primary mb-2"><i class="fas fa-money-bill-wave"></i><em class="ml-2"></em>Comprar</button>
                                  <p class="text-muted small-xl">ao comprar você receberá o premium automaticamente, durante 30 dias!</p>               
                                </div>
                            </form>                                                                                                                                                           
                        </div>
                    </div>
                </div>
            </div>
</section>

